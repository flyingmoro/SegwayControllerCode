        \}\par
    \}\par
    if (verify) free(verifyBuffer);\par
    if (useProgMem) free(progBuffer);\par
    return true;\par
\}\par
bool MPU6050::writeProgMemoryBlock(const uint8_t *data, uint16_t dataSize, uint8_t bank, uint8_t address, bool verify) \{\par
    return writeMemoryBlock(data, dataSize, bank, address, verify, true);\par
\}\par
