void MPU6050::readMemoryBlock(uint8_t *data, uint16_t dataSize, uint8_t bank, uint8_t address) \{\par
    setMemoryBank(bank);\par
    setMemoryStartAddress(address);\par
    uint8_t chunkSize;\par
    for (uint16_t i = 0; i < dataSize;) \{\par
        // determine correct chunk size according to bank position and data size\par
        chunkSize = MPU6050_DMP_MEMORY_CHUNK_SIZE;\par
\par
        // make sure we don't go past the data size\par
        if (i + chunkSize > dataSize) chunkSize = dataSize - i;\par
\par
        // make sure this chunk doesn't go past the bank boundary (256 bytes)\par
        if (chunkSize > 256 - address) chunkSize = 256 - address;\par
\par
        // read the chunk of data as specified\par
        I2Cdev::readBytes(devAddr, MPU6050_RA_MEM_R_W, chunkSize, data + i);\par
        \par
        // increase byte index by [chunkSize]\par
        i += chunkSize;\par
\par
        // uint8_t automatically wraps to 0 at 256\par
        address += chunkSize;\par
\par
        // if we aren't done, update bank (if necessary) and address\par
        if (i < dataSize) \{\par
            if (address == 0) bank++;\par
            setMemoryBank(bank);\par
            setMemoryStartAddress(address);\par
        \}\par
    \}\par
\}\par
bool MPU6050::writeMemoryBlock(const uint8_t *data, uint16_t dataSize, uint8_t bank, uint8_t address, bool verify, bool useProgMem) \{\par
    setMemoryBank(bank);\par
    setMemoryStartAddress(address);\par
    uint8_t chunkSize;\par
    uint8_t *verifyBuffer;\par
    uint8_t *progBuffer=0;\par
    uint16_t i;\par
    uint8_t j;\par
    if (verify) verifyBuffer = (uint8_t *)malloc(MPU6050_DMP_MEMORY_CHUNK_SIZE);\par
    if (useProgMem) progBuffer = (uint8_t *)malloc(MPU6050_DMP_MEMORY_CHUNK_SIZE);\par
    for (i = 0; i < dataSize;) \{\par
        // determine correct chunk size according to bank position and data size\par
        chunkSize = MPU6050_DMP_MEMORY_CHUNK_SIZE;\par
\par
        // make sure we don't go past the data size\par
        if (i + chunkSize > dataSize) chunkSize = dataSize - i;\par
\par
        // make sure this chunk doesn't go past the bank boundary (256 bytes)\par
        if (chunkSize > 256 - address) chunkSize = 256 - address;\par
        \par
        if (useProgMem) \{\par
            // write the chunk of data as specified\par
            for (j = 0; j < chunkSize; j++) progBuffer[j] = pgm_read_byte(data + i + j);\par
        \} else \{\par
            // write the chunk of data as specified\par
            progBuffer = (uint8_t *)data + i;\par
        \}\par
\par
        I2Cdev::writeBytes(devAddr, MPU6050_RA_MEM_R_W, chunkSize, progBuffer);\par
\par
        // verify data if needed\par
        if (verify && verifyBuffer) \{\par
            setMemoryBank(bank);\par
            setMemoryStartAddress(address);\par
            I2Cdev::readBytes(devAddr, MPU6050_RA_MEM_R_W, chunkSize, verifyBuffer);\par
            if (memcmp(progBuffer, verifyBuffer, chunkSize) != 0) \{\par
                /*Serial.print("Block write verification error, bank ");\par
